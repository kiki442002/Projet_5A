    <conditions>
        <condition id="TZ Non-secure">
            <description>TrustZone (Non-secure)</description>
            <accept Dsecure="Non-secure"/>
            <accept Dsecure="TZ-disabled"/>
        </condition>
        <condition id="Cortex-M Devices">
            <description>Cortex-M processor based device: Cortex-M0, Cortex-M0+, Cortex-M4 and Cortex-M33</description>
            <require Dname="STM32*"/>
            <require condition="TZ Non-secure"/>
            <accept Dcore="Cortex-M0+"/>
            <accept Dcore="Cortex-M4"/>
            <accept Dcore="Cortex-M7"/>
            <accept Dcore="Cortex-M33"/>
        </condition>
        <!-- HAL IPs -->
        <condition id="STM32Cube HAL - SPI">
            <description>STM32Cube HAL - SPI</description>
            <require condition="Cortex-M Devices"/>
            <accept Cclass="Device" Cgroup="SPI" Csub="HAL"/> <!-- STM32CubeMX: SPI HAL -->
            <accept Cclass="Device" Cgroup="STM32Cube HAL" Csub="SPI"/> <!-- Keil: SPI HAL -->
        </condition>
        <condition id="STM32Cube HAL - QSPI">
            <description>STM32Cube HAL - QSPI</description>
            <require condition="Cortex-M Devices"/>
            <accept Cclass="Device" Cgroup="QSPI" Csub="HAL"/> <!-- STM32CubeMX: QSPI HAL -->
            <accept Cclass="Device" Cgroup="STM32Cube HAL" Csub="QSPI"/> <!-- Keil: QSPI HAL -->
        </condition>
        <condition id="STM32Cube HAL - OSPI">
            <description>STM32Cube HAL - OSPI</description>
            <require condition="Cortex-M Devices"/>
            <accept Cclass="Device" Cgroup="OSPI" Csub="HAL"/> <!-- STM32CubeMX: OSPI HAL -->
            <accept Cclass="Device" Cgroup="STM32Cube HAL" Csub="OSPI"/> <!-- Keil: OSPI HAL -->
        </condition>
        <condition id="STM32Cube HAL - FMC">
            <description>STM32Cube HAL - FSMC/FMC</description>
            <require condition="Cortex-M Devices"/>
            <accept Cclass="Device" Cgroup="FSMC" Csub="HAL"/> <!-- STM32CubeMX: FSMC HAL -->
            <accept Cclass="Device" Cgroup="STM32Cube HAL" Csub="FSMC"/> <!-- Keil: FSMC HAL -->
            <accept Cclass="Device" Cgroup="FMC" Csub="HAL"/> <!-- STM32CubeMX: FMC HAL -->
            <accept Cclass="Device" Cgroup="STM32Cube HAL" Csub="FMC"/> <!-- Keil: FMC HAL -->
        </condition>
        <condition id="Custom LCD - SPI">
            <description>Custom LCD - SPI</description>
            <require condition="Cortex-M Devices"/>
            <require condition="STM32Cube HAL - SPI"/>
            <require condition="Custom Board"/>
            <deny Cbundle="DISPLAY" Cclass="Board Part" Cgroup="LCD" Csub="ILI9341" />
            <deny Cbundle="DISPLAY" Cclass="Board Part" Cgroup="LCD" Csub="ST7789V" />
        </condition>
        <condition id="Custom LCD - FMC">
            <description>Custom LCD - FSMC/FMC</description>
            <require condition="Cortex-M Devices"/>
            <require condition="STM32Cube HAL - FMC"/>
            <require condition="Custom Board"/>
            <deny Cbundle="DISPLAY" Cclass="Board Part" Cgroup="LCD" Csub="ILI9341" />
            <deny Cbundle="DISPLAY" Cclass="Board Part" Cgroup="LCD" Csub="ST7789V" />
        </condition>
        <condition id="ILI9341 - SPI">
            <description>ILI9341 - SPI</description>
            <require condition="Cortex-M Devices"/>
            <require condition="STM32Cube HAL - SPI"/>
            <deny Cbundle="DISPLAY" Cclass="Board Part" Cgroup="LCD" Csub="ST7789V" />
        </condition>
        <condition id="ILI9341 - FMC">
            <description>ILI9341 - FSMC/FMC</description>
            <require condition="Cortex-M Devices"/>
            <require condition="STM32Cube HAL - FMC"/>
            <deny Cbundle="DISPLAY" Cclass="Board Part" Cgroup="LCD" Csub="ST7789V" />
        </condition>
        <condition id="ST7789V - SPI">
            <description>ST7789V - SPI</description>
            <require condition="Cortex-M Devices"/>
            <require condition="STM32Cube HAL - SPI"/>
            <deny Cbundle="DISPLAY" Cclass="Board Part" Cgroup="LCD" Csub="ILI9341" />
        </condition>
        <condition id="ST7789V - FMC">
            <description>ST7789V - FSMC/FMC</description>
            <require condition="Cortex-M Devices"/>
            <require condition="STM32Cube HAL - FMC"/>
            <deny Cbundle="DISPLAY" Cclass="Board Part" Cgroup="LCD" Csub="ILI9341" />
        </condition>
        <condition id="Custom Memory - SPI">
            <description>Custom Memory - SPI</description>
            <require condition="Cortex-M Devices"/>
            <require condition="STM32Cube HAL - SPI"/>
            <require condition="Custom Board"/>
            <deny Cbundle="DISPLAY" Cclass="Board Part" Cgroup="Memory" Csub="MX25L6433F" />
        </condition>
        <condition id="Custom Memory - QSPI">
            <description>Custom Memory - QSPI</description>
            <require condition="Cortex-M Devices"/>
            <require condition="STM32Cube HAL - QSPI"/>
            <require condition="Custom Board"/>
            <deny Cbundle="DISPLAY" Cclass="Board Part" Cgroup="Memory" Csub="MX25L6433F" />
        </condition>
        <condition id="Custom Memory - OSPI">
            <description>Custom Memory - OSPI</description>
            <require condition="Cortex-M Devices"/>
            <require condition="STM32Cube HAL - OSPI"/>
            <require condition="Custom Board"/>
            <deny Cbundle="DISPLAY" Cclass="Board Part" Cgroup="Memory" Csub="MX25L6433F" />
        </condition>
        <condition id="MX25L6433F - SPI">
            <description>MX25L6433F - SPI</description>
            <require condition="Cortex-M Devices"/>
            <require condition="STM32Cube HAL - SPI"/>
        </condition>
        <!-- Disable support of QSPI IP for MX25L6433F
        <condition id="MX25L6433F - QSPI">
            <description>MX25L6433F - QSPI</description>
            <require condition="Cortex-M Devices"/>
            <require condition="STM32Cube HAL - QSPI"/>
        </condition>
        -->
        <condition id="MX25L6433F - OSPI">
            <description>MX25L6433F - OSPI</description>
            <require condition="Cortex-M Devices"/>
            <require condition="STM32Cube HAL - OSPI"/>
        </condition>
        <!-- CUSTOM Board -->
        <condition id="Custom Board">
            <deny Cbundle="DISPLAY" Cclass="Extension Board" Cgroup="GFX01M1"/>
            <deny Cbundle="DISPLAY" Cclass="Extension Board" Cgroup="GFX01M2"/>
            <deny Cbundle="DISPLAY" Cclass="Extension Board" Cgroup="GFX02Z1"/>
        </condition>
        <condition id="Custom Board Parts">
            <description>Custom board</description>
            <require condition="Cortex-M Devices"/>
            <require condition="Custom Board"/>
            <accept Cbundle="DISPLAY" Cclass="Board Part" Cgroup="LCD"/>
            <accept Cbundle="DISPLAY" Cclass="Board Part" Cgroup="Memory"/>
        </condition>
        <!-- X-NUCLEO-GFX01M1 -->
        <condition id="GFX01M1 LCD Parts">
            <require condition="Cortex-M Devices"/>
            <require Cbundle="DISPLAY" Cclass="Board Part" Cgroup="LCD" Csub="ILI9341" Cvariant="SPI"/>
        </condition>
        <condition id="GFX01M1 Memory Parts">
            <require condition="Cortex-M Devices"/>
            <require Cbundle="DISPLAY" Cclass="Board Part" Cgroup="Memory" Csub="MX25L6433F" Cvariant="SPI"/>
        </condition>
        <condition id="GFX01M1 Board Parts">
            <description>GFX01M1 board require both ILI9341/SPI and MX25L6433F/SPI Board Parts</description>
            <require condition="GFX01M1 LCD Parts"/>
            <require condition="GFX01M1 Memory Parts"/>
            <deny Cbundle="DISPLAY" Cclass="Board Part" Cgroup="LCD" Csub="Custom"/>
            <deny Cbundle="DISPLAY" Cclass="Board Part" Cgroup="Memory" Csub="Custom"/>
            <deny Cbundle="DISPLAY" Cclass="Extension Board" Cgroup="GFX01M2"/>
            <deny Cbundle="DISPLAY" Cclass="Extension Board" Cgroup="GFX02Z1"/>
            <deny Cbundle="DISPLAY" Cclass="Device" Cgroup="Application" Cvariant="Custom_Application"/>
        </condition>
        <!-- X-NUCLEO-GFX01M2 -->
        <condition id="GFX01M2 LCD Parts">
            <require condition="Cortex-M Devices"/>
            <accept Cclass="Board Part" Cgroup="LCD" Csub="ILI9341" Cvariant="SPI"/>
            <accept Cclass="Board Part" Cgroup="LCD" Csub="ST7789V" Cvariant="SPI"/>
        </condition>
        <condition id="GFX01M2 Memory Parts">
            <require condition="Cortex-M Devices"/>
            <require Cbundle="DISPLAY" Cclass="Board Part" Cgroup="Memory" Csub="MX25L6433F" Cvariant="SPI"/>
        </condition>
        <condition id="GFX01M2 Board Parts">
            <description>GFX01M2 board require both ILI9341/SPI or ST7789V/SPI and MX25L6433F/SPI Board Parts</description>
            <require condition="GFX01M2 LCD Parts"/>
            <require condition="GFX01M2 Memory Parts"/>
            <deny Cbundle="DISPLAY" Cclass="Board Part" Cgroup="LCD" Csub="Custom"/>
            <deny Cbundle="DISPLAY" Cclass="Board Part" Cgroup="Memory" Csub="Custom"/>
            <deny Cbundle="DISPLAY" Cclass="Extension Board" Cgroup="GFX01M1"/>
            <deny Cbundle="DISPLAY" Cclass="Extension Board" Cgroup="GFX02Z1"/>
            <deny Cbundle="DISPLAY" Cclass="Device" Cgroup="Application" Cvariant="Custom_Application"/>
        </condition>
        <!-- X-NUCLEO-GFX02Z1 -->
        <condition id="GFX02Z1 LCD Parts">
            <require condition="Cortex-M Devices"/>
            <accept Cbundle="DISPLAY" Cclass="Board Part" Cgroup="LCD" Csub="ILI9341" Cvariant="FMC"/>
            <accept Cbundle="DISPLAY" Cclass="Board Part" Cgroup="LCD" Csub="ST7789V" Cvariant="FMC"/>
        </condition>
        <condition id="GFX02Z1 Memory Parts">
            <require condition="Cortex-M Devices"/>
            <!-- Disable support of QSPI IP for MX25L6433F
            <accept Cbundle="DISPLAY" Cclass="Board Part" Cgroup="Memory" Csub="MX25L6433F" Cvariant="QSPI"/>
            <accept Cbundle="DISPLAY" Cclass="Board Part" Cgroup="Memory" Csub="MX25L6433F" Cvariant="OSPI"/>
            -->
            <require Cclass="Board Part" Cgroup="Memory" Csub="MX25L6433F" Cvariant="OSPI"/>
            <!-- Disable support of QSPI IP for MX25L6433F -->
        </condition>
        <condition id="GFX02Z1 Board Parts">
            <description>GFX02Z1 board require both ILI9341/FMC or ST7789V/FMC and MX25L6433F/Quad SPI Board Parts</description>
            <require condition="GFX02Z1 LCD Parts"/>
            <require condition="GFX02Z1 Memory Parts"/>
            <deny Cbundle="DISPLAY" Cclass="Board Part" Cgroup="LCD" Csub="Custom"/>
            <deny Cbundle="DISPLAY" Cclass="Board Part" Cgroup="Memory" Csub="Custom"/>
            <deny Cbundle="DISPLAY" Cclass="Extension Board" Cgroup="GFX01M1"/>
            <deny Cbundle="DISPLAY" Cclass="Extension Board" Cgroup="GFX01M2"/>
            <deny Cbundle="DISPLAY" Cclass="Device" Cgroup="Application" Cvariant="Custom_Application"/>
        </condition>
        <!-- Device Applications -->
        <condition id="Custom Applications">
            <description>Condition for Custom board</description>
            <require condition="Custom Board Parts"/>
        </condition>
        <condition id="GFX01M1 Applications">
            <description>Condition for X-NUCLEO-GFX01M1 Expansion board</description>
            <require condition="GFX01M1 Board Parts"/>
            <require Cbundle="DISPLAY" Cclass="Extension Board" Cgroup="GFX01M1"/>
        </condition>
        <condition id="GFX01M2 Applications">
            <description>Condition for X-NUCLEO-GFX01M2 Expansion board</description>
            <require condition="GFX01M2 Board Parts"/>
            <require Cbundle="DISPLAY" Cclass="Extension Board" Cgroup="GFX01M2"/>
        </condition>
        <condition id="GFX02Z1 Applications">
            <description>Condition for X-NUCLEO-GFX02Z1 Expansion board</description>
            <require condition="GFX02Z1 Board Parts"/>
            <require Cbundle="DISPLAY" Cclass="Extension Board" Cgroup="GFX02Z1"/>
        </condition>
    </conditions>
