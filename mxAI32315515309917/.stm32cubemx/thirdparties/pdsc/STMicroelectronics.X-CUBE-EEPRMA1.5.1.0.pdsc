	  <deny Cbundle="EEPROM" Cclass="Board Extension" Cgroup="EEPRMA2"/>
	  <deny Cbundle="EEPROM" Cclass="Board Extension" Cgroup="EEICA1"/>
	  <deny Cbundle="EEPRMA_Applications" Cclass="Device" Cgroup="Application" Cvariant="EEPRMA2_EEPROMRW"/>
	  <deny Cbundle="EEPRMA_Applications" Cclass="Device" Cgroup="Application" Cvariant="EEICA1_EEPROMRW"/>
	</condition>
	
