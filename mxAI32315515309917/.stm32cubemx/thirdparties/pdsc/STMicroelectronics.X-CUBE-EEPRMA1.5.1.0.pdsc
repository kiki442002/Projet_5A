    </condition>
    <condition id="CM7_IAR">
      <require Dcore="Cortex-M7"/>
      <require Tcompiler="IAR"/>
    </condition>
    <condition id="CM7_Keil">
	  <require Dcore="Cortex-M7"/>
      <require Tcompiler="ARMCC"/>
    </condition>
	
	<condition id="QUADSPI Condition">		
	  <description>Cortex-M Device and board setup (X-NUCLEO-PGEEZ1)</description>
	  <require Cbundle="EEPROM" Cclass="Board Extension" Cgroup="PGEEZ1"/>
	  <require Cclass="Device" Cgroup="QSPI" Csub="HAL"/>
	  <deny Cbundle="EEPROM" Cclass="Board Extension" Cgroup="EEPRMA2"/>
	  <deny Cbundle="EEPROM" Cclass="Board Extension" Cgroup="EEICA1"/>
	  <deny Cbundle="EEPROM" Cclass="Board Part" Cgroup="M95P32"/>
	  <deny Cbundle="EEPROM" Cclass="Board Part" Cgroup="M24"/>
	  <deny Cbundle="EEPROM" Cclass="Board Part" Cgroup="M95"/>
	  <deny Cbundle="EEPROM" Cclass="Board Part" Cgroup="M24256E-F"/>
	  <deny Cbundle="EEPROM" Cclass="Board Part" Cgroup="M24M01E-F"/>
	  <deny Cbundle="SPI_Interface" Cclass="PGEEZ1" Cgroup="SPI"/>
    </condition>
	
	<condition id="SPI Condition">		
	  <description>Cortex-M Device and board setup (X-NUCLEO-PGEEZ1)</description> 
	  <require Cbundle="EEPROM" Cclass="Board Extension" Cgroup="PGEEZ1"/>
	  <require Cclass="Device" Cgroup="SPI" Csub="HAL"/>
	  <deny Cbundle="EEPROM" Cclass="Board Extension" Cgroup="EEPRMA2"/>
	  <deny Cbundle="EEPROM" Cclass="Board Extension" Cgroup="EEICA1"/>
