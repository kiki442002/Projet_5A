	  <deny Cbundle="EEPROM" Cclass="Board Part" Cgroup="M24M01E-F"/>
	  <deny Cbundle="QUADSPI_Interface" Cclass="PGEEZ1" Cgroup="QDSPI"/>
    </condition>
    	
	<condition id="EEPRMA2 Condition">		
	  <description>Cortex-M Device and board setup (X-NUCLEO-EEPRMA2)</description> 
	  <require condition="Cortex-M Device"/>   
      <deny Cbundle="EEPROM" Cclass="Board Part" Cgroup="M24"/>
	  <deny Cbundle="EEPROM" Cclass="Board Part" Cgroup="M95"/>
	  <deny Cbundle="EEPROM" Cclass="Board Part" Cgroup="M95P32"/>
	  <deny Cbundle="EEPROM" Cclass="Board Part" Cgroup="M24256E-F"/>
	  <deny Cbundle="EEPROM" Cclass="Board Part" Cgroup="M24M01E-F"/>
	  <deny Cbundle="EEPROM" Cclass="Board Extension" Cgroup="PGEEZ1"/>
	  <deny Cbundle="EEPROM" Cclass="Board Extension" Cgroup="EEICA1"/>
	  <deny Cbundle="EEPRMA_Applications" Cclass="Device" Cgroup="Application" Cvariant="PGEEZ1_EEPROMRW"/>
	  <deny Cbundle="EEPRMA_Applications" Cclass="Device" Cgroup="Application" Cvariant="EEICA1_EEPROMRW"/>
    </condition>
	
	<condition id="PGEEZ1 Condition">		
